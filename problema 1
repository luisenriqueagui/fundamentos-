# fundamentos-programacion

#include <iostream>
#include "conio.h"

using namespace std;
int main()
{
	float a,b,c,d,r=0;
	cout<<"ingrese la primera variable:";
	cin>>a;
	cout<<"ingrese la segunda variable:";
	cin>>b;
	cout<<"ingrese la tercera variable:";
	cin>>c;
	cout<<"ingrese la cuarta variable:";
	cin>>d;
    r = (a+b)/(c+d);
	cout<<"el resultado es :"<<r<<endl;

	return 0;
}
