# fundamentos-programacion
#include "stdafx.h"
#include <iostream>
#include <conio.h>
using namespace std;

int main()
{ int  n,numeros[100],menor=100;
	
cout<<"Ingrese el numero de elementos del vector:"<<endl;
	cin>>n;
	for(int i=0;i<n;i++)
	{ cout<<"Ingrese el elemento:["<<i<<"]:";
		cin>>numeros[i];
		if(numeros[i]<menor)
		{menor=numeros[i];}}
	cout<<"El numero menor es:"<<menor;


	getch();
}
